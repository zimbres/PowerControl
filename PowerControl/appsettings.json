{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "Configuration": {
    "Url": "https://127.0.0.1/get_power_command",
    "Data": "shutdown",
    "Delay": 60,
    "Command": "shutdown",
    "Arguments": "/s /t 60",
    "IamAliveUrl": "https://127.0.0.1/iam_alive",
    "IamAliveEnabled": true
  }
}
